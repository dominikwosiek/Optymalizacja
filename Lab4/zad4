A = ([-1, 1], [1,0])
b = (0,2)
c = (0,1)
P = InteractiveLPProblemStandardForm(A, b, c, ["x1", "x2"])
show(P)
P.plot()
D = P.initial_dictionary()
show(D)
print D.possible_entering()
D.enter("x2")
print D.possible_leaving()
D.leave("x3")
show(D)
D.update()
show(D)
D.enter("x1")
print D.possible_leaving()
D.leave("x4")
show(D)
D.update()
show(D)
P.plot()
